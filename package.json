{
  "name": "jira-ticket-assigner",
  "version": "1.1.2",
  "private": true,
  "description": "Automatically assign jira tickets to PRs using github actions",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/jira-ticket-assigner.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Steven Masala",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/parser": "^4.3.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.10.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.0.2",
    "jest": "^26.5.0",
    "jest-circus": "^26.5.1",
    "js-yaml": "^3.14.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
